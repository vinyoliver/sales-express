<div class="row">
    {{#each data}}
    <div class="col-3">
        <div class="card" >
            <div class="card-body">
                <h5 class="card-title">{{this.description}}</h5>
                <h6 class="card-subtitle mb-2 text-muted">$ {{this.price}}</h6>
                <p class="card-text">{{this.description}}</p>
                <a href="{{this.url}}" target="_blank" class="card-link">Click here</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>